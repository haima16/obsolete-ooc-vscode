{
	"name": "obsolete-ooc",
	"displayName": "obsolete-ooc",
	"description": "新时代下无用的OOC生成器，兼容mcfunction文件",
	"version": "0.1.1",
	"engines": {
		"vscode": "^1.46.0"
	},
	"keywords": ["minecraft", "command", "mcfunction", "ooc"],
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:obsolete-ooc.generate"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "obsolete-ooc.generate",
				"title": "generate"
			}
		],
		"keybindings": [
			{
				"command": "obsolete-ooc.generate",
				"key": "ctrl+k ctrl+g",
				"mac": "cmd+k cmd+g",
				"when": "editorFocus && resourceLangId == mcfunction"
			}
		],
		"snippets": [
            {
                "language": "mcfunction",
				"path": "./snippets/config.json"
            }
        ]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"license": "MIT",
	"repository": {
        "type": "git",
        "url": ""
    },
    "homepage": ""
}
